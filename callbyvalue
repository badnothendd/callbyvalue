#include <stdio.h>
#include <stdlib.h>


 // call by value: a ile b ye x ve y nin değerlerini verdikten sorna x ile y nin değişmemesi. sadece a ve b ye değer atıyor o kadar. 
 
 void degerdegistir(int a, int b) {
 	
 	 int gecici=a;
 	 a= b;
 	 b= gecici;
 	 
 	 printf("a: %d, b:%d\n", a,b);
 	 
 
 }

int main() {
	
	 int x=5, y=6;
	 
	 degerdegistir(x,y);
	 
	 printf("x:%d, y:%d", x,y);
	
	
	return 0;
}
